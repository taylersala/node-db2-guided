{
  "name": "node-db2-guided",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "ketchup": "git fetch && git reset --hard origin/lecture",
    "up": "knex migrate:up",
    "latest": "knex migrate:latest",
    "down": "knex migrate:down",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run latest && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/node-db2-guided.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "nodemon": "^2.0.21"
  }
}
